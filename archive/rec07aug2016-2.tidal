d3 $ palindrome $ density 1.5 $ sound "can electro"

d3 silence

d4 $ slow 2 $ sound "pacman:1*2"
|+| speed "-0.5 -0.6"
|+| accelerate "-1"
|+| begin "0.8"
|+| gain "0.7"

d4 silence

hush
