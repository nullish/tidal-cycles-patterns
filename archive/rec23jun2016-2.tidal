d1 $ palindrome $ superimpose (|+| speed "0.4 0.6 0.2 0.5") $ slow 4 $ sound "blip*24"
|+| speed "-1.5"

d2 $ slow 2 $ spin 2 $ striate 2 $ sound "bass"
|+| speed "1"
|+| accelerate "1"

d1 $ whenmod 8 6 (trunc 0.25) $ sound "tb303:1*4"
|+| speed "1.2 1 0.8 0.7"
|+| accelerate "-1 -0.9"
|+| gain "1.2"

d3 $ every 4 (|+| speed "1 -1 1 1") $ superimpose (density 1.5) $ sound "bd sn:2 [bd ~ sn:1]/2"
|+| gain "0.9"
